(procedure foo ([stem "foo"])
	   ([n int     "n" "int"]
	    [a pointer "a" "double *"]
	    [b pointer "b" "double *"])
   (load COMPLEX () c ([reg a]))
   (loop () (i [const 0] [reg n])
     (load COMPLEX () va ([reg a]))
     (op pointer-add () (a) ([reg a] [const (size-of COMPLEX)]))
     (load COMPLEX () vb ([reg b]))
     (op pointer-add () (b) ([reg b] [const (size-of COMPLEX)]))
     (op complex-madd () (c) ([reg c] [reg va] [reg vb])))
   (store COMPLEX () ([reg a]) [reg c]))
