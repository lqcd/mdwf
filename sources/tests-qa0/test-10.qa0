(procedure qcd-f ([stem "qcd"]
                  count-flops)
              ([res    pointer   "struct Fermion *" "result"]
               [U      pointer   "struct SU3 *"     "U"]
               [scale  double    "double"           "scale"]
               [src    pointer   "struct Fermion *" "source"])
   (load qcd-fermion () x ([reg src]))
   (load qcd-fermion () v ([reg U]))
   (op qcd-maddf () (z) ([reg scale] [reg v] [reg x]))
